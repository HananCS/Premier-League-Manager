# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

GET      /                           controllers.FrontendController.index()

GET      /clubs                      controllers.ClubMatchController.getClubs()

GET      /clubs2                     controllers.ClubMatchController.getClubs2()

GET      /clubs3                     controllers.ClubMatchController.getClubs3()

GET      /matches                    controllers.ClubMatchController.getMatches()

# Test post request
POST    /api/postTest               controllers.HomeController.postTest()



